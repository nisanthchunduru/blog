{
  "name": "blog",
  "version": "1.0.0",
  "description": "Source code of my blog (https://nisanthchunduru.github.io)",
  "main": "src/server.ts",
  "scripts": {
    "start": "./node_modules/.bin/ts-node src/server.ts",
    "dev": "nodemon",
    "tw:build": "npx tailwindcss -i ./static/css/tailwind.css -o ./static/css/tailwind.compiled.css",
    "tw:watch": "npx tailwindcss -i ./static/css/tailwind.css -o ./static/css/tailwind.compiled.css -w",
    "docker:start": "docker compose up --build --pull always",
    "docker:push": "docker build -t nisanth074/blog:latest . --push",
    "docker:push:x86": "docker buildx build --platform linux/amd64 -t nisanth074/blog:latest . --push",
    "deploy": "./deploy.sh"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@notionhq/client": "^2.2.15",
    "date-fns": "^3.0.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "file-system-cache": "^2.4.7",
    "js-yaml": "^4.1.0",
    "liquidjs": "^10.8.3",
    "lodash": "^4.17.21",
    "nodemon": "^3.0.2",
    "slugify": "^1.6.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.12",
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.6",
    "tailwindcss": "^3.4.3"
  }
}
